@article{Cartry2017,
abstract = {This paper deals with a study of H − /D − negative ion surface production on diamond in low pressure H 2 /D 2 plasmas. A sample placed in the plasma is negatively biased with respect to plasma potential. Upon positive ion impacts on the sample, some negative ions are formed and detected according to their mass and energy by a mass spectrometer placed in front of the sample. The experimental methods developed to study negative ion surface production and obtain negative ion energy and angle distribution functions are first presented. Different diamond materials ranging from nanocrystalline to single crystal layers, either doped with boron or intrinsic, are then investigated and compared with graphite. The negative ion yields obtained are presented as a function of different experimental parameters such as the exposure time, the sample bias which determines the positive ion impact energy and the sample surface temperature. It is concluded from these experiments that the electronic properties of diamond materials, among them the negative electron affinity, seem to be favourable for negative-ion surface production. However, the negative ion yield decreases with the plasma induced defect density.},
author = {Cartry, Gilles and Kogut, Dmitry and Achkasov, Kostiantyn and Layet, Jean-Marc and Farley, Thomas and Gicquel, Alix and Achard, Jocelyn and Brinza, Ovidiu and Bieber, Thomas and Khemliche, Hocine and Roncin, Philippe and Simonin, Alain},
file = {:home/tfarley/OneDrive/4-Papers/Document{\_}management/mendeley{\_}watched/Cartry{\_}2017{\_}New{\_}J.{\_}Phys.{\_}19{\_}025010.pdf:pdf},
isbn = {0963-0252},
journal = {New J. Phys.},
number = {2},
pages = {25010},
publisher = {IOP Publishing},
title = {{Alternative solutions to caesium in negative-ion sources: a study of negative-ion surface production on diamond in H 2 /D 2 plasmas}},
url = {http://stacks.iop.org/1367-2630/19/i=2/a=025010},
volume = {19},
year = {2017}
}
@article{Militello2016a,
abstract = {Four numerical codes are employed to investigate the dynamics of scrape-off layer flaments in tokamak relevant conditions. Experimental measurements were taken in the MAST device using visual camera imaging, which allows the evaluation of the perpendicular size and velocity of the flaments, as well as the combination of density and temperature associated with the perturbation. A new algorithm based on the light emission integrated along the feld lines associated with the position of the flament is developed to ensure that it is properly detected and tracked. The flaments are found to have velocities of the order of − 1 km s 1 , a perpendicular diameter of around 2–3 cm and a density amplitude 2–3.5 times the background plasma. 3D and 2D numerical codes (the STORM module of BOUT++, GBS, HESEL and TOKAM3X) are used to reproduce the motion of the observed flaments with the purpose of validating the codes and of better understanding the experimental data. Good agreement is found between the 3D codes. The seeded flament simulations are also able to reproduce the dynamics observed in experiments with accuracy up to the experimental errorbar levels. In addition, the numerical results showed that flaments characterised by similar size and light emission intensity can have quite different dynamics if the pressure perturbation is distributed differently between density and temperature components. As an additional beneft, several observations on the dynamics of the flaments in the presence of evolving temperature felds were made and led to a better understanding of the behaviour of these coherent structures.},
author = {Militello, F. and Walkden, N. R. and Farley, T. and Gracias, W. A. and Olsen, J. and Riva, F. and Easy, L. and Fedorczak, N. and Lupelli, I. and Madsen, J. and Nielsen, A. H. and Ricci, P. and Tamain, P. and Young, J.},
doi = {10.1088/0741-3335/58/10/105002},
file = {:home/tfarley/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Militello et al. - 2016 - Multi-code analysis of scrape-off layer filament dynamics in MAST.pdf:pdf},
issn = {13616587},
journal = {Plasma Phys. Control. Fusion},
keywords = {MAST,code validation,filaments,scrape off layer},
number = {10},
pages = {105002},
publisher = {IOP Publishing},
title = {{Multi-code analysis of scrape-off layer filament dynamics in MAST}},
url = {http://stacks.iop.org/0741-3335/58/i=10/a=105002?key=crossref.c1ba900da67b8f4f9726fbdcbe2ea76b},
volume = {58},
year = {2016}
}
