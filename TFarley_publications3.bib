author = {Arnoux, G. and Farley, T. and Silva, C. and Devaux, S. and Firdaouss, M. and Frigione, D. and Goldston, R.J. and Gunn, J. and Horacek, J. and Jachmich, S. and Lomas, P.J. and Marsen, S. and Matthews, G.F. and Pitts, R.A. and Stamp, M. and Stangeby, P.C.},
