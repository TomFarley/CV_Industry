doi = {10.1016/j.nme.2016.10.024},
file = {:home/tfarley/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Walkden et al. - 2016 - Identification of intermittent transport in the scrape-off layer of MAST through high speed imaging.pdf:pdf},
